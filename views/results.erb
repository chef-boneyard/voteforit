<table class='table table-hover table-striped'>
    <% settings.choices.each do |id, text| %>
        <tr>
            <th><%= text %></th>
            <td><%= @votes[id] || 0 %></td>
            <td><%= '#' * Integer(@votes[id] || 0) %></td>
        </tr>
    <% end %>
</table>
<p><a href="/">Cast more votes!</a></p>
